{
  "name": "vis-timeline-arrows",
  "version": "5.0.1",
  "description": "Package to easily draw lines to connect items in the vis Timeline module. Fork from @alisqi and @javdome v4.1.1",
  "main": "arrows.js",
  "types": "arrows.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "generateTypes": "tsc --allowJs -d --emitDeclarationOnly --lib es2015,dom arrows.js",
    "watch": "tsc --allowJs -d --emitDeclarationOnly --lib es2015,dom arrows.js --watch",
    "prepack": "npm run generateTypes",
    "serve": "lite-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jan-dolejsi/timeline-arrows.git"
  },
  "keywords": [
    "vis",
    "vis-timeline"
  ],
  "author": "Javi Domenech",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jan-dolejsi/timeline-arrows/issues"
  },
  "homepage": "https://github.com/jan-dolejsi/timeline-arrows#readme",
  "devDependencies": {
    "lite-server": "^2.6.1",
    "typescript": "^5.2.2"
  }
}
